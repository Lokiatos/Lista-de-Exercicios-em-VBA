Private Sub LT01_EstSeq05_Click()

'Objetivo: Resolver equações do 2º grau - raízes reais'
'Programador: Hugo Leça Ribeiro'

Dim A As Double
Dim B As Double
Dim c As Double
Dim delta As Double
Dim x1 As Double
Dim x2 As Double
A = InputBox("Digite o valor de A")
B = InputBox("Digite o valor de B")
c = InputBox("Digite o valor de C")
delta = ((B * B) - 4 * A * c)
x1 = (-B + Sqr(delta)) / (2 * A)
x2 = (-B - Sqr(delta)) / (2 * A)
MsgBox "O valor de X1 é: " & x1
MsgBox "O valor de X2 é: " & x2

End Sub
